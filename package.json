	"scripts": {
		"ng": "ng",
		"start": "node dist/server/index.js",
		"start:dev": "run-p client-ng2:live server:live",
		"build": "ng build",
		"test": "mocha",
		"dev:run": "run-p client-ng2:live node dist/server/index.js",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"preinstall": "",
		"postinstall": "",
		"build:server": "./node_modules/.bin/tsc --module commonjs --sourceMap --target ES5",
		"build:web": "node --max_old_space_size=102400 ./node_modules/@angular/cli/bin/ng build --aot -prod",
		"server:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./server/index.ts",
		"client-ng2:live": "ng build --watch",
		"client-pug:live": "npm run pug-it:w",
		"pug-it": "pug src",
		"pug-it:w": "pug src -P -w"
	},
	"private": true,
